var
  b, i, j: Integer;
  p: TPoint;

begin
  for j := 0 to 1000 do
  begin
    b := BitmapFromString(501, 501, '');
    for i := 0 to 9999 do
    begin
      p := RndBoxPointG(Box(0, 0, 500, 500), 3);
      FastSetPixel(b, p.X, p.Y, 255);
    end;
    DebugBitmap(B);
    FreeBitmap(B);
  end;
end.